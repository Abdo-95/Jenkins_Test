pipeline {
    agent any
    stages {
        stage('Welcome Step') {
            steps { 
                echo 'Welcome to Jenkins Test'
            }
        }
        stage('Build') {
            steps { 
                echo 'Building completed '
            }
        }
        stage('Test') {
            steps { 
                echo 'Testing completed'
            }
        }      
        stage('Deploy') {
            steps { 
                echo 'Test Deployment completed '
            }
        }          
    }
}
